{
  "compilerOptions": {
    "outDir": "./dist/",
    "typeRoots": ["./src/js/engine/globals.d.ts"],
    "baseUrl": ".",
    "paths": {
      "engine/*": ["src/js/engine/*"]
    },
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "lib": ["es2019", "dom"],
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
